{
  "name": "clawless",
  "version": "0.1.0",
  "description": "Open source personal agent runtime with hybrid intelligence",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"next dev\" \"tsx watch src/runtime/index.ts\"",
    "build": "tsc && next build",
    "start": "concurrently \"next start\" \"node dist/runtime/index.js\"",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "db:init": "node scripts/init-db.js",
    "quickstart": "npm run db:init && npm run dev"
  },
  "keywords": [
    "agent",
    "automation",
    "ai",
    "llm",
    "ollama",
    "claude",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@anthropic-ai/sdk": "^0.17.0",
    "better-sqlite3": "^9.6.0",
    "zod": "^3.23.0",
    "node-cron": "^3.0.3",
    "octokit": "^3.1.2",
    "@slack/web-api": "^7.0.0",
    "dotenv": "^16.4.0",
    "yaml": "^2.4.0",
    "nanoid": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/better-sqlite3": "^7.6.10",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.4.0",
    "tsx": "^4.7.0",
    "vitest": "^1.5.0",
    "@testing-library/react": "^14.3.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "prettier": "^3.2.0",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
